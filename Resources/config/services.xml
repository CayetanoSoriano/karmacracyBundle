<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="cayetanosoriano_kcy.class">kcy\kcy</parameter>
    </parameters>

    <services>
        <service id="kcy" class="%cayetanosoriano_kcy.class%">
            <argument>%cayetanosoriano_karmacracy.appkey%</argument>
            <argument>%cayetanosoriano_karmacracy.keypass%</argument>
        </service>
    </services>
</container>
